<%= form_for(writing) do |f| %>
    <%= f.text_field :title, id: 'editor-title', placeholder: 'Title', class: 'ui huge header', autocomplete: 'off', spellcheck: 'false' %>
    <%= f.text_area :body, class: 'fr-view', data: { acl: @hash[:acl], AWSAccessKeyId: @hash[:access_key], policy: @hash[:policy], signature: @hash[:signature] } %>
<% end %>
